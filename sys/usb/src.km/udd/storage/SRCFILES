# This file gets included by the Makefile in this directory to determine
# the files that should go only into source distributions.

HEADER = \
	part.h \
	scsi.h \
	vars.h \
	xhdi.h

COBJS = \
	usb_storage.c \
	xhdi.c

SSOBJS = \
	bios.S \
	debug2.S

TOSCOBJS = \
        crtinit.c

TOSSSOBJS = \
        entry.S \
        setstack.S

SRCFILES = $(HEADER) $(SSOBJS) $(COBJS) $(TOSCOBJS) $(TOSSSOBJS)
